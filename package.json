{
  "name": "graphiql-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix --color",
    "format:fix": "prettier . --write",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "jest --coverage --silent",
    "coverage": "istanbul cover node_modules/jest/bin/jest.js --coverage && istanbul report --include coverage/*/src/**/*.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@graphiql/react": "^0.20.2",
    "@hookform/resolvers": "^3.3.2",
    "@mui/material": "^5.15.3",
    "@mui/styled-engine-sc": "^6.0.0-alpha.11",
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@uiw/react-codemirror": "^4.21.21",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "firebase": "^10.7.1",
    "graphiql": "^3.0.10",
    "graphql": "^16.8.1",
    "isomorphic-fetch": "^3.0.0",
    "ra-data-graphql": "^4.16.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-redux": "^9.0.4",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.1.6",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.4.1",
    "msw": "^2.0.11",
    "prettier": "^2.6.2",
    "sass": "^1.69.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": "babel-jest",
      "^.+\\.scss$": "jest-transform-stub"
    },
    "testEnvironmentOptions": {
      "customExportConditions": []
    },
    "setupFiles": [
      "./jest.polyfills.js"
    ]
  }
}
